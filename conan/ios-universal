armv7_CFLAGS=-fPIC -DPIC -arch armv7 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk -mios-version-min=10.0 -Os
armv7_LDFLAGS=-mios-version-min=10.0 -arch armv7 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk
armv7_TRIPLE=arm-apple-darwin

x86_64_SDKROOT=/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator.sdk
x86_64_CC=ccache /usr/bin/clang -arch x86_64
x86_64_CXX=ccache /usr/bin/clang++ -arch x86_64
