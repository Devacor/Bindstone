[settings]
compiler=apple-clang
compiler.version=9.0
compiler.libcxx=libc++
os=iOS
os.version=11.0
arch=universal
build_type=Release

[env]
SDKROOT=/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk
CC=ccache /usr/bin/clang -arch arm64
CXX=ccache /usr/bin/clang++ -arch arm64
CXXFLAGS=-fPIC -DPIC -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk -mios-version-min=10.0 -Os
CFLAGS=-fPIC -DPIC -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk -mios-version-min=10.0 -Os
LDFLAGS=-mios-version-min=10.0 -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk
TRIPLE=arm-apple-darwin

arm64_SDKROOT=/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk
arm64_CC=ccache /usr/bin/clang -arch arm64
arm64_CXX=ccache /usr/bin/clang++ -arch arm64
arm64_CXXFLAGS=-fPIC -DPIC -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk -mios-version-min=10.0 -Os
arm64_CFLAGS=-fPIC -DPIC -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk -mios-version-min=10.0 -Os
arm64_LDFLAGS=-mios-version-min=10.0 -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk
arm64_TRIPLE=arm-apple-darwin

