[settings]
compiler=apple-clang
compiler.version=9.0
compiler.libcxx=libc++
os=iOS
os.version=11.2
arch=universal
build_type=Release

[env]
SDKROOT=/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk
CC=ccache /usr/bin/clang -arch arm64
CXX=ccache /usr/bin/clang++ -arch arm64
CXXFLAGS=-fPIC -DPIC -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk -mios-version-min=11.2 -Os -fembed-bitcode
CFLAGS=-fPIC -DPIC -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk -mios-version-min=11.2 -Os -fembed-bitcode
LDFLAGS=-mios-version-min=11.2 -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk
TRIPLE=arm-apple-darwin

arm64_SDKROOT=/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk
arm64_CC=ccache /usr/bin/clang -arch arm64
arm64_CXX=ccache /usr/bin/clang++ -arch arm64
arm64_CXXFLAGS=-fPIC -DPIC -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk -mios-version-min=11.2 -Os -fembed-bitcode
arm64_CFLAGS=-fPIC -DPIC -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk -mios-version-min=11.2 -Os -fembed-bitcode
arm64_LDFLAGS=-mios-version-min=11.2 -arch arm64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk
arm64_TRIPLE=arm-apple-darwin

x86_64_SDKROOT=/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator.sdk
x86_64_CC=ccache /usr/bin/clang -arch x86_64
x86_64_CXX=ccache /usr/bin/clang++ -arch x86_64
x86_64_CXXFLAGS=-fPIC -DPIC -arch x86_64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator.sdk -mios-version-min=11.2 -Os -fembed-bitcode
x86_64_CFLAGS=-fPIC -DPIC -arch x86_64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator.sdk -mios-version-min=11.2 -Os -fembed-bitcode
x86_64_LDFLAGS=-mios-version-min=11.2 -arch x86_64 -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/iPhoneSimulator.platform/Developer/SDKs/iPhoneSimulator.sdk
x86_64_TRIPLE=x86_64-apple-darwin
